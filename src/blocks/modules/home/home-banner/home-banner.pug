section.homepage-banner
    .homepage-banner__slider.js-homepage-banner__slider.splide
        .splide__track
            ul.splide__list
                li.splide__slide.homepage-banner__slide
                    .homepage-banner__image
                        picture
                            source(srcset="img/home/banner-1.webp" type="image/webp" media="(min-width: 641px)")
                            source(srcset="img/home/banner.jpg" type="image/jpeg" media="(min-width: 641px)")
                            source(srcset="img/home/banner.webp" type="image/webp")
                            img(src="img/home/banner.jpg")
                    .homepage-banner__content
